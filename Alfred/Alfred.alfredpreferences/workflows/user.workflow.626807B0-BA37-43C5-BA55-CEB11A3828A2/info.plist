				<key>script</key>
				<string>function run(argv) {
  var query = argv[0];

  // Setup
  chrome = Application("Google Chrome");
  systemEvents = Application('System Events');
  systemEvents.includeStandardAdditions = true;
  var app = Application.currentApplication()
  app.includeStandardAdditions = true
  const activeTab = chrome.windows[0].activeTab
  const { url, title: tabTitle } = activeTab

  // Grab the original clipboard for reference
  const originalClipboard = systemEvents.theClipboard()

  // Copy the current chrome selection to the clipboard
  chrome.copySelection(activeTab);
  const selection = app.theClipboard();
  console.log('selection', selection);

  // Determine if Chrome actually had a selection by seeing if the clipboard changed
  const chromeHasSelection = selection !== originalClipboard
  console.log('chromeHasSelection', chromeHasSelection)

  // Determine the title based on if there was a selection or not
  let title
  if (chromeHasSelection) {
    title = selection.trim();
  } else {
    title = tabTitle()
  }

  return `[${title}](${url()})`;
}</string>
