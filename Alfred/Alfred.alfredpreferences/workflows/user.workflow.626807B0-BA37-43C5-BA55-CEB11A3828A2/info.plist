				<key>script</key>
				<string>function run(argv) {
  var query = argv[0];

  chrome = Application("Google Chrome");
  activeTab = chrome.windows[0].activeTab
  chrome.copySelection(activeTab);

  systemEvents = Application('System Events');
  systemEvents.includeStandardAdditions = true;  
  linkTitle = systemEvents.theClipboard();

  return linkTitle;
}</string>
