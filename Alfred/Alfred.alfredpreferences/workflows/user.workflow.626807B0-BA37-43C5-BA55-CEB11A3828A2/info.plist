				<key>script</key>
				<string>function run(argv) {
  var query = argv[0];

  const chrome = Application("Google Chrome")
  const activeTab = chrome.windows[0].activeTab
  const { url, title: tabTitle } = activeTab

  const givenTitle = query;
  const title = givenTitle || tabTitle()

  return `[${title}](${url()})`;
}</string>
