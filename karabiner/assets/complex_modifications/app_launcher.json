{
  "title": "Quick app launching",
  "rules": [
    {
      "description": "Quick app launching for frequently used apps",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Google Chrome'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Visual Studio Code'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Slack'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": {
              "mandatory": ["control"]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Terminal'"
            }
          ]
        }
      ]
    }
  ]
}
