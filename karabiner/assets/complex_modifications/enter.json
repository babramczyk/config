{
  "title": "Enter",
  "rules": [
    {
      "description": "Enter is Ctrl if held, Enter if alone",
      "manipulators": [
        {
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "right_control",
              "lazy": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 150
          },
          "type": "basic"
        }
      ]
    }
  ]
}
